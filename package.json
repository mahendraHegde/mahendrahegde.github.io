{
  "name": "portfolio",
  "version": "0.1.0",
  "homepage": "https://mahendrahegde.github.io/portfolio",
  "private": true,
  "dependencies": {
    "install": "^0.12.2",
    "node-sass": "^4.14.1",
    "npm": "^6.14.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.3",
    "react-typing-effect": "^2.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "plugins": [
      "prettier"
    ]
  },
  "eslintIgnore": [
    "node_modules/*",
    ".github"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-config-prettier": "^6.11.0",
    "prettier-eslint": "^11.0.0"
  }
}
