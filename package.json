{
  "name": "portfolio",
  "version": "0.2.0",
  "homepage": "https://mahendrahegde.github.io/portfolio",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.5.3",
    "install": "^0.12.2",
    "node-sass": "^4.14.1",
    "npm": "^7.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx .",
    "lint-fix": "eslint --fix --ext .js,.jsx ."
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "plugins": [
      "prettier"
    ]
  },
  "eslintIgnore": [
    "node_modules/*",
    ".github",
    "build/*"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier-eslint": "^12.0.0"
  }
}
